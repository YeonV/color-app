{
  "name": "color-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "next start",
    "build:server": "tsc --project tsconfig.server.json",
    "start:server": "node dist/server/server.js",
    "start:dev": "run-script-os",
    "start:dev:win": "nodemon --watch server.ts --exec \\\"npm run build:server && npm run start:server\\\"",
    "start:dev:nix": "nodemon --watch server.ts --exec \"npm run build:server && npm run start:server\"",
    "dev": "concurrently \"npm run start:dev\" \"next dev\"",
    "lint": "next lint"
  },
  "dependencies": {
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "cors": "^2.8.5",
    "eslint-config-next": "^13.4.4",
    "express": "^4.18.2",
    "memory-cache": "^0.2.0",
    "next": "13.4.4",
    "react": "18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "18.2.0",
    "run-script-os": "^1.1.6",
    "socket.io": "^4.6.2",
    "socket.io-client": "^4.6.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/memory-cache": "^0.2.2",
    "@types/node": "^20.2.5",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "concurrently": "^8.1.0",
    "eslint": "^8.42.0",
    "nodemon": "^2.0.22",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
